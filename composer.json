{
    "name": "grossberger-georg/run-script",
    "type": "typo3-cms-extension",
    "description": "Run a server-side script via the backend",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "GrossbergerGeorg\\RunScript\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrossbergerGeorg\\RunScript\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "typo3/cms-backend": "^12.4",
        "typo3/cms-core": "^12.4",
        "typo3/cms-extbase": "^12.4",
        "typo3/cms-fluid": "^12.4"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "run_script"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.3",
        "phpunit/phpunit": "^9.5",
        "mikey179/vfsstream": "^1.6"
    },
    "config": {
        "allow-plugins": {
            "typo3/class-alias-loader": false,
            "typo3/cms-composer-installers": false
        }
    }
}
